server {
    listen 80;
    access_log  /var/log/nginx/example.log;

    location /static {
        alias /app/blog/static/;
    }

    location / {
        proxy_pass http://127.0.0.1:1342;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
}
